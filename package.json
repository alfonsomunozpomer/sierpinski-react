{
  "name": "sierpinski-react",
  "version": "1.0.0",
  "description": "Sierpinski triangle explorer in React with <canvas>",
  "main": "lib/index.js",
  "scripts": {
    "dist": "NODE_ENV=production webpack --optimize-minimize",
    "prepublishOnly": "rm -rf lib && babel src -d lib --copy-files",
    "test": "jest --coverage"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <a.munozpomer.fuentes@icloud.ac.uk>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/alfonsomunozpomer/sierpinski-react.git"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "enzyme": "^2.9.1",
    "jest": "^21.1.0",
    "react-test-renderer": "^15.6.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  }
}
